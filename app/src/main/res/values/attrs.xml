<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ViewfinderView">
        <attr name="inner_width" format="dimension|reference"/>
        <attr name="inner_height" format="dimension|reference"/>
        <attr name="inner_margintop" format="dimension|reference"/>
        <attr name="leftMargin" format="dimension|reference"/>
        <attr name="rightMargin" format="dimension|reference"/>
        <attr name="inner_corner_color" format="color"/>
        <attr name="inner_corner_length" format="dimension"/>
        <attr name="inner_corner_width" format="dimension"/>
        <attr name="inner_scan_bitmap" format="reference"/>
        <attr name="inner_scan_speed" format="integer"/>
        <attr name="inner_scan_iscircle" format="boolean"/>
        <!--扫描线的周期默认是1000毫秒-->
        <attr name="inner_scan_line_period" format="integer"/>
    </declare-styleable>

    <declare-styleable name="MyViewfinderView">
        <attr name="scan_width" format="dimension|reference"/>
        <attr name="scan_height" format="dimension|reference"/>
        <attr name="marginTop" format="dimension|reference"/>
        <attr name="left_Margin" format="dimension|reference"/>
        <attr name="right_Margin" format="dimension|reference"/>
        <attr name="scanBitmap" format="reference"/>
        <attr name="scanPeriod" format="integer"/>
        <attr name="scanLine" format="reference"/>
    </declare-styleable>

</resources>